{
    "name": "Imbalance_Places_LT_TADE_test_time_training",
    "n_gpu": 2,

    "arch": {
        "type": "ResNet152Model",
        "args": {
            "num_classes": 365,
            "reduce_dimension": false,
            "use_norm": true,
            "returns_feat": true,
            "share_layer3": false,
            "num_experts": 3
        }
    },
    "data_loader": {
        "type": "Places_LT",
        "args":{
            "data_dir": "./data/Place365",
            "batch_size": 128,
            "num_workers": 8
        }
    },
    "optimizer": {
        "type": "SGD",
        "args":{
            "lr": 0.01,
            "share_lr": 0.001,
            "weight_decay": 4e-4,
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "add_extra_info": true, 
    "metrics": [
        "accuracy"
    ],
    "epochs": 5 
}
